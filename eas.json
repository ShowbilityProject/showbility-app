{
  "cli": {
    "version": ">= 3.15.0",
    "appVersionSource": "remote",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "image": "auto"
      },
      "ios": {
        "image": "auto"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "image": "auto"
      },
      "ios": {
        "image": "auto"
      }
    },
    "production": {
      "channel": "production",
      "autoIncrement": true,
      "android": {
        "image": "auto"
      },
      "ios": {
        "image": "auto"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
